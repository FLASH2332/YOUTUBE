<!DOCTYPE html>
<html>

<head>
    <title>Youtube</title>
    <link rel="icon" type="image/x-icon" href="/static/images/icons/favicon.ico">
    <link rel="stylesheet" href="/static/css,js/video/BasePageStyle.css">
    <link rel="stylesheet" href="/static/css,js/common.css">
</head>

<body>
    <nav class="navbar">
        <div class="toggle-btn" id="toggleBtn">
            <span></span>
            <span></span>
            <span></span>
        </div>
        <img src="/static/images/icons/logo.png" class="logo" alt="logo">
        <div class="search-box" title="Search Box">
            <input type="text" class="search-bar" placeholder="search">
            <button class="search-btn"><img src="/static/images/icons/search.PNG" alt="Search"></button>
        </div>
        <div id="theme">
            <input type="checkbox" class="checkbox" id="navbar-theme">
        </div>
        <div class="user-options">
            <a href="/youtube-clone/<%= currentuserid %>/channels/<%= currentuserid %>">
                <div class="user-dp">
                    <img src="<%= video.selfprofilepic %>" class="icon" alt="User" title="Your Profile">
                </div>
            </a>
        </div>

    </nav>
    <div class="side-bar">
        <a href="/youtube-clone/<%= currentuserid %>">
            <div class="links">
                <img src="/static/images/icons/home.PNG" alt="Home" title="Home">
                <span class="link-text">Home</span>
            </div>
        </a>
        <hr class="separator">
        <a href="/youtube-clone/<%= currentuserid %>/subscriptions">
            <div class="links">
                <img src="/static/images/icons/subscription.PNG" alt="Subscription" title="Subscriptions">
                <span class="link-text">Subscriptions</span>
            </div>
        </a>
        <a href="/youtube-clone/<%= currentuserid %>/channels/<%= currentuserid %>">
            <div class="links">
                <img src="/static/images/icons/your-video.PNG" alt="Your Video" title="Your Video">
                <span class="link-text">Your Video</span>
            </div>
        </a>
        <div id="theme">
            <input type="checkbox" class="checkbox" id="insidebar-theme">
        </div>
    </div>


    <div id="cover" class="Cover">
        <p>kkk</p>
    </div>

    <div id="content">
        <div id="mainContent">
            <div id="mainVideo">
                <div class="video-container">
                    <% console.log(video.thumbnail); %>
                        <img src="<%= video.thumbnail %>" id="video-thumbnail">
                        <video id="video">
                            <source src="<%= video.video %>" type="video/mp4">
                        </video>
                        <div id="screen">
                            <div id="videoCover"><span></span></div>
                            <div class="controls">
                                <div id="top">
                                    <div class="playback-line">
                                        <div class="progress-bar"></div>
                                    </div>
                                </div>
                                <div id="bottom">
                                    <div class="left">
                                        <button id="skipminus-10" class="fa-solid fa-backward">
                                            <img title="Rewind 10"
                                                src="data:image/png;base64,">
                                        </button>
                                        <button id="play-pause" class="fa-solid fa-play">
                                            <i><img title="Play" id="pbutton"
                                                    src="data:image/png;base64,">
                                            </i>
                                        </button>
                                        <button id="skip-10" class="fa-solid fa-forward">
                                            <i><img title="Skip 10"
                                                    src="data:image/png;base64,"></i>
                                        </button>
                                        <div class="video-timer">
                                            <span id="current-time">00:00</span>
                                            <span id="separator">/</span>
                                            <span id="max-duration">00:00</span>
                                        </div>
                                        <div class="volume-container">
                                            <div>
                                                <button id="mute">
                                                    <img src="/static/IMAGES/IMAGES/download (4).jpg">
                                                    <i class="fas fa-volume-up"></i>
                                                </button>
                                            </div>
                                            <input type="range" id="volume" min="0" max="1" step="0.01" value="1">
                                        </div>
                                    </div>
                                    <div class="right">
                                        <button id="fullscreen">
                                            <img title="Fullscreen" id="fullscreenButton" src="/static/IMAGES/IMAGES/download (3).jpg"/>
                                            <i class="fa-solid fa-backward"><img title="Fullscreen"
                                                    id="fullscreenButton" src="" /></i>
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>
                </div>
            </div>
            <div id="description">
                <div id="mainVideoInfo">
                    <h1 id="mainVideoTitle">
                        <%= video.videotitle %>
                    </h1>
                    <div>
                        <div id="videoProfile">
                            <img style="cursor: pointer;" onclick='window.location = "/youtube-clone/<%= currentuserid %>/channels/<%= video.userid %>"' src="<%= video.channelprofilepic %>" />
                            <p><span id="mainChannelName">
                                    <%= video.channelname %>
                                </span><br /><span id="mainSubscribers">
                                    <%= video.channelsubs %> subscribers
                                </span></p>
                            <% if (currentuserid !==video.userid) {%>
                                <div id="subscribe"><button type="button" id="subscribeButton">Subscribe</button></div>
                                <% } %>
                        </div>
                        <% if (currentuserid !==video.userid) {%>
                            <div id="likeDislike">
                                <button type="button" id="likeButton"><img src="/static/images/icons/liked video.png" />
                                    <span id="likes">
                                        <%= video.likes %>
                                    </span>
                                </button>
                                <button type="button" id="dislikeButton"><img
                                        src="/static/images/icons/liked video.png" /><span id="likes">
                                        <%= video.dislikes %>
                                    </span></button>
                            </div>
                            <% } %>
                    </div>
                </div>
                <div id="descriptionMain">
                    <p><span id="videoViews">
                            <%= video.views %> views
                        </span><span> </span><span id="videoTime">DATE</span></p>
                    <p id="videoTags">
                        <% for (let tag of video.tags) { %>
                            #<%= tag %>
                                <% } %>
                    </p>
                    <p id="descriptionText">
                        <%= video.description %>
                    </p>
                    <button id="descriptionExtend" type="button">Show more</button>
                </div>
            </div>
            <hr />
            <div id="toggleComments">
                <button id="commentsToggle" type="button">Comments</button>
            </div>
            <div id="comments">
                <div class="comment addComment">
                    <div class="commentProfilePic">
                        <img src="">
                    </div>
                    <div class="commentDetails">
                        <form onsubmit="return addCommentP(event)" action="" method="post">

                            <!--<input contenteditable="true" id="submittedComment" type="text" id="fname" maxlength="150" placeholder="Add a comment...." name="user_text">-->
                            <span contenteditable="true" id="submittedComment">Add a comment....</span>
                            <button type="submit">Submit</button>
                        </form>
                    </div>
                </div>
                <div class="commentSeparator"></div>
            </div>
        </div>
        <div id="subContent">
            <% for (let video of sideVideos) { %>
                <div class="videoTag">
                    <a href="/youtube-clone/<%= currentuserid %>/videos/<%= video._id %>"><img
                            src="<%= video.thumbnail %>"></a>
                    <div class="videoInfo">
                        <a class="Title" href="/youtube-clone/<%= currentuserid %>/videos/<%= video._id %>">
                            <p>
                                <%= video.videotitle %>
                            </p>
                        </a>
                        <a class="subInfo" href="/youtube-clone/<%= currentuserid %>/channels/<%= video.userid %>">
                            <p>
                                <%= video.channelname %>
                            </p>
                        </a>
                        <div class="bottomVideoInfo">
                            <p class="subInfo bottomInfo">
                                <%= video.views %> views
                            </p>
                        </div>
                    </div>
                </div>
                <% } %>
        </div>
    </div>

    </div>
    <script src="/static/css,js/video/BasePage.js"></script>
</body>

</html>